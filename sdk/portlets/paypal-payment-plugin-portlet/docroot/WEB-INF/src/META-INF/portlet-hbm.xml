<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.beorn.paypalpaymentplugin.model.Token" />
	<class name="com.beorn.paypalpaymentplugin.model.impl.TokenImpl" table="PaypalPaymentPlugin_Token">
		<cache usage="read-write" />
		<id name="tokenId" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="transactionId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>